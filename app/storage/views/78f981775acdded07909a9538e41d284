		<script>
			// This is only for JavaScript that doesn't work in the footer

			// disable enter key submit for certain forms
			function disableEnterKey(e) {
				var key;
				if (window.event)
					key = window.event.keyCode;
				//IE
				else
					key = e.which;
				//firefox
				return (key != 13);
			}

		</script>
